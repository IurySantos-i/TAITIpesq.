<% content_for :head do %>
  <% if @css %>
    <%= stylesheet_link_tag @css, media: "all" %>
    <%= stylesheet_link_tag 'embed', media: "all" %>
  <% end %>
  <% if params.include? "css_content" %>
    <style>
      <%= params["css_content"] %>
    </style>
  <% end %>
<% end -%>
<%= render "tools/container" %>
<%= ahoy_track_tag("embedded_view", @actionPage.id) %>
